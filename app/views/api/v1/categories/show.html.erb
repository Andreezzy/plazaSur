<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @category.name %>
</p>

<p>
  <strong>Description:</strong>
  <%= @category.description %>
</p>

<p>
  <strong>Product:</strong>
  <ul>
    <% @category.products.each do |product| %>
      <li><%= product.name %></li>
    <% end %>
  </ul>
</p>

<%= link_to 'Edit', edit_api_v1_category_path(@category) %> |
<%= link_to 'Back', api_v1_categories_path %>
