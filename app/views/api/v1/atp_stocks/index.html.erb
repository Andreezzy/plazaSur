<p id="notice"><%= notice %></p>

<h1>Atp Stocks</h1>

<table>
  <thead>
    <tr>
      <th>Min stock</th>
      <th>Max stock</th>
      <th>Product</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @atp_stocks.each do |atp_stock| %>
      <tr>
        <td><%= atp_stock.min_stock %></td>
        <td><%= atp_stock.max_stock %></td>
        <td><%= atp_stock.product %></td>
        <td><%= link_to 'Show', atp_stock %></td>
        <td><%= link_to 'Edit', edit_atp_stock_path(atp_stock) %></td>
        <td><%= link_to 'Destroy', atp_stock, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Atp Stock', new_atp_stock_path %>
